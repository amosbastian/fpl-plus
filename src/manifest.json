{
  "author": "Amos Bastian",
  "name": "FPL Plus",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "A browser extension for the Fantasy Premier League",
  "homepage_url": "https://github.com/amosbastian/fpl-plus",
  "browser_action": {
    "default_title": "FPL Plus",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
    "content_scripts": [
      {
        "matches": ["*://fantasy.premierleague.com/a/team/my*"],
        "js": ["myTeam.bundle.js"],
        "run_at": "document_idle"
      },
      {
        "matches": ["https://fantasy.premierleague.com/a/leagues/standings/*/classic",
          "https://fantasy.premierleague.com/a/leagues/standings/*/classic*"],
        "js": ["classicLeague.bundle.js"],
        "run_at": "document_idle"
      }
    ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  }
}
